{
  "presets": [
    "es2015"
  ],
  "comments": false,
  "env": {
    "main": {
      "presets": [
        [
          "env",
          {
            "targets": {
              "node": 7
            }
          }
        ],
        "stage-0"
      ]
    },
    "renderer": {
      "presets": [
        [
          "env",
          {
            //            "modules": false
          }
        ],
        "stage-0"
      ]
    },
    "web": {
      "presets": [
        [
          "env",
          {
            //            "modules": false
          }
        ],
        "stage-0"
      ]
    }
  },
  "plugins": [
    "transform-runtime",
    [
      "transform-es2015-template-literals",
      {
        "loose": true,
        "spec": true
      }
    ]
  ]
}